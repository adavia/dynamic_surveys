%h1.text-center.m-t-0
  = link_to "#{@user} Profile", [:edit, :admin, @user]

%p.text-center
  = @user.email

- unless @user == current_user
  %h3.text-center
    = link_to "Archive User", [:archive, :admin, @user], method: :patch,
      data: { confirm: "Are you sure you want to archive this user?"}
